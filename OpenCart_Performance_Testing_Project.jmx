          <stringProp name="query">--  CLEANUP SCRIPT FOR OPEN CART LOAD/STRESS TESTS (OpenCart 4.1.0.3 Schema)
-- Deletes all data created during a JMeter run 
-- Safe for your opencart_db database

-- Clear cart data (incomplete sessions)
DELETE FROM oc_cart;

-- Remove order dependencies (children first)
DELETE FROM oc_order_total 
WHERE order_id IN (
  SELECT order_id FROM oc_order WHERE comment LIKE CONCAT(&apos;%&apos;, &apos;${run_name}&apos;, &apos;%&apos;)
);

DELETE FROM oc_order_product 
WHERE order_id IN (
  SELECT order_id FROM oc_order WHERE comment LIKE CONCAT(&apos;%&apos;, &apos;${run_name}&apos;, &apos;%&apos;)
);

DELETE FROM oc_order_history 
WHERE order_id IN (
  SELECT order_id FROM oc_order WHERE comment LIKE CONCAT(&apos;%&apos;, &apos;${run_name}&apos;, &apos;%&apos;)
);

-- Delete parent order rows
DELETE FROM oc_order 
WHERE comment LIKE CONCAT(&apos;%&apos;, &apos;${run_name}&apos;, &apos;%&apos;);

-- Clean related activity logs of deleted customers
DELETE FROM oc_customer_activity 
WHERE customer_id NOT IN (SELECT customer_id FROM oc_customer);

-- Finally, remove the test customers themselves
DELETE FROM oc_customer WHERE email LIKE &apos;user_%@mail.com&apos;;

